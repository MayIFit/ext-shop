{
    "name": "mayifit/ext-shop",
    "description": "MayIFit E-Commerce extension",
    "keywords": ["e-Commerce", "laravel", "php", "shop", "webshop"],
    "type": "project",
    "authors": [
        {
            "name": "MayIFit",
            "email": "contact@mayifit.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "MayIFit\\Extension\\Shop\\": "src/",
            "MayIFit\\Extension\\Shop\\Database\\Seeds\\": "src/Database/Seeds/",
            "MayIFit\\Extension\\Shop\\Facades\\": "src/Facades/",
            "MayIFit\\Extension\\Shop\\Models\\": "src/Models/",
            "MayIFit\\Extension\\Shop\\Models\\Pivots\\": "src/Models/Pivots/",
            "MayIFit\\Extension\\Shop\\Policies\\": "src/Policies/"
        }
    },
    "require": {
        "php": "^7.2",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MayIFit\\Extension\\Shop\\ShopServiceProvider"
            ],
            "aliases": {
                "Product": "MayIFit\\Extension\\Shop\\Facades\\Product"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
