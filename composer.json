{
    "name": "mayifit/ext-shop",
    "description": "MayIFit E-Commerce extension",
    "keywords": ["E-Commerce", "laravel", "php", "shop", "webshop"],
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/MayIFit/core-permission"
        },
        {
            "type": "vcs",
            "url": "https://github.com/MayIFit/core-translation"
        }
    ],
    "authors": [
        {
            "name": "MayIFit",
            "email": "contact@mayifit.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "MayIFit\\Extension\\Shop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MayIFit\\Extension\\Shop\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/contracts": "^6.9 || ^7.0",
        "illuminate/database": "^6.9 || ^7.0",
        "illuminate/support": "^6.9 || ^7.0",
        "nesbot/carbon": "^1.26.0 || ^2.0",
        "mayifit/core-permission": "^1.0",
        "mayifit/core-translation": "^1.0",
        "nuwave/lighthouse": "^4.12",
        "mll-lab/graphql-php-scalars": "^3.0",
        "laravel/sanctum": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0 || ^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "MayIFit\\Extension\\Shop\\ShopServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test:unit": "phpunit --colors=always --testsuite Unit",
        "test:feature": "phpunit --colors=always --testsuite Feature"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
